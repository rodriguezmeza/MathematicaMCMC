    RowBox[{
    "3.91", " ", "Mpc", "\n", "#", " ", "Rad", " ", "Vobs", " ", "errV", " ", 
     "Vgas", " ", "Vdisk", " ", "Vbul", " ", "SBdisk", " ", "SBbul", "\n", 
     "#", " ", "kpc", " ", 
